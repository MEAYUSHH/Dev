console.log(arr2);


for objects
let obj ={
    name:"jj",
    add:{
        country:"india",
        state:{
            code:"dl",
            pin:"110085"
        }

    }

}
// let obj2=obj;
// obj2.name="asdf";

// console.log(obj);
// console.log(obj2);

//using spread operator

let obj2={...obj,add:{...obj.add}};
obj2.name="asdf";
obj2.add.country="US";

console.log(obj);
console.log(obj2);
