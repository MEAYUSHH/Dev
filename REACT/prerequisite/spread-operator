//for array

let arr=[1,2,3];

// let arr2=arr;

// arr2.push(4);

// console.log(arr);
// console.log(arr2);

// console.log(arr);
// console.log(...arr);//list mai kar diya arr ko

let arr2=[...arr];
arr2.push[6];
